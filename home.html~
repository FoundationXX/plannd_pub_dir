  <!--Testing stage scripts for subscription response======start here--> 
  <!--   DOES NOT WORK because of the same origin policy, even JSONP only supports get request, but not post request
         btu why does form submit work????-->
<script>
  $(document).ready(function () {
  $('#mc-embedded-subscribe-form').submit(function(event) {
    if(event) event.preventDefault();
    subscribe($('#mc-embedded-subscribe-form'));
});
    
   function subscribe(form) {
  $.ajax({
    type: form.attr('method'),
    url: form.attr('action'),
    data: form.serialize(),
    cache       : false,
    dataType    : 'jsonp',
    jsonp: "c",
    contentType: "application/json; charset=utf-8",
    error       : function(err) { 
     // Although according to jquery docs, this is never called for cross-domain JSONP requests, the following is just for precaution
    $('#mce-error-response').html('<span class="alert">Could not connect to server. Please try again later.</span>');},
    success     : function(data) { 
      $('#mce-response').css({"color": "#fff", "font-size": "200%"});
      console.log("data is: " + JSON.stringify(data));
      if (data.result != "success") {
      	
      	console.log('data.msg is: '+ data.msg);
      	if (~((data.msg).indexOf('subscribe'))){
      		var message = data.msg;
      	}      	
      	else{
        	var message =(data.msg==='0'?data.msg:"Sorry, something was not right. Did you fill in your email address correctly?");
        }
        console.log("something is wrong: "+ message);
        $('#mce-response').html(message);        
       } 
 
      else {
        $('#form-container').css("display","none");
        $('#star-reminder').css("display","none");
        var message = data.msg;
        console.log("sucess: " + message);
        $('#mce-response').html('<span style="color:#DE5452;">Thank you for subscribing!</span>'+'<br><span class="success">' + message + '<br><span style="color:#DE5452;">Wish you a day of romance!</span></span>');
      }
    }
  });
  }
  });
  <!--All Rights Reserved, author>>>>Liang Liu-->
  </script>  
 
  <!--scripts for subscription response======end here-->   
